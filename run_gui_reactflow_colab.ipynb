        "# 録画や録音の保存時のオートダウンロード設定"
      ],
      "metadata": {
        "id": "iNbI9M0hZufC"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import json\n",
        "from pathlib import Path\n",
        "\n",
        "# 読み込み\n",
        "config_path = Path(\"config.json\")\n",
        "with config_path.open(\"r\", encoding=\"utf-8\") as f:\n",
        "    config = json.load(f)\n",
        "\n",
        "# 値を変更\n",
        "config.setdefault(\"auto_download\", {})[\"video\"] = True\n",
        "config.setdefault(\"auto_download\", {})[\"wav\"] = True\n",
        "config.setdefault(\"auto_download\", {})[\"capture\"] = True\n",
        "config.setdefault(\"auto_download\", {})[\"text\"] = True\n",
        "\n",
        "# 保存\n",
        "with config_path.open(\"w\", encoding=\"utf-8\") as f:\n",
        "    json.dump(config, f, indent=2, ensure_ascii=False)"
      ],
      "metadata": {
        "id": "e7JlwxNsZu9r"
      },
      "execution_count": 5,
      "outputs": []
